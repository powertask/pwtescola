<%= simple_form_for(@contract) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :unit %>
    <%= f.input :amount_unit %>
    <%= f.input :amount_client %>
    <%= f.input :status %>
    <%= f.association :debtor %>
    <%= f.input :client_ticket_quantity %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
