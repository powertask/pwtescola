<%= simple_form_for(@ticket) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :description %>
    <%= f.input :amount %>
    <%= f.association :debtor %>
    <%= f.input :document_number %>
    <%= f.input :due_at %>
    <%= f.input :charge %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
