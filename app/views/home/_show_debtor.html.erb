<div class='subtitle'> <%= t('home.debtor.show') %> </div>
<% unless @debtor.nil? %>
  <div class='section'>
    <div class='label1 top req'> <%="ID" %>: </div>    <%= @debtor.id %>
    <div class='label1 top req'> <%=t("debtor.origin_code") %>: </div>    <%= @debtor.origin_code %>
    <div class='label1 top req'> <%=t("customer.name") %>: </div> <%= @debtor.customer.name %>
    <div class='label1 top req'> <%=t("debtor.name") %>: </div>  <%= link_to(@debtor.name, debtor_path(@debtor.id)) %>
    <div class='label1 top req'> <%=t("phone") %>: </div>         <%= @debtor.phone_number %>
    
    <div class='label1 top req'> <%=t("cnpj_cpf") %>: </div>      <%= @debtor.cpf.present? ? @debtor.cpf : @debtor.cnpj %>
    <div class='label1 top req'> <%=t("address") %>: </div>       <%= @debtor.address %>
    <div class='label1 top req'> <%=t("address_complement") %>: </div>    <%= @debtor.address_complement %>
    <div class='label1 top req'> <%=t("neighborhood") %>: </div>  <%= @debtor.neighborhood %>
    <div class='label1 top req'> <%=t("zipcode") %>: </div>       <%= @debtor.zipcode %>
    <div class='label1 top req'> <%=t("city") %>: </div> <%= @debtor.city_name  %>
    <div class='label1 top req'> <%=t("state") %>: </div>         <%= @debtor.state %>
  </div>
<% end %> 
